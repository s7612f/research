{
    "llm": {
        "base_url": "http://localhost:11434",
        "model": "dolphin-mixtral:8x7b",
        "temperature": 0.7,
        "max_tokens": 4096,
        "timeout": 120,
        "retry_attempts": 3
    },
    "embedding": {
        "model": "all-MiniLM-L6-v2",
        "dimension": 384
    },
    "research": {
        "max_iterations": 100,
        "min_sources": 10,
        "max_sources": 500,
        "depth_levels": 3,
        "enable_controversial": true,
        "fact_check_threshold": 0.8
    },
    "scraping": {
        "rate_limit": 10,
        "timeout": 30,
        "user_agents": [],
        "proxy_list": []
    },
    "database": {
        "path": "data/research.db",
        "backup_interval": 3600,
        "connection_pool_size": 10
    }
}
